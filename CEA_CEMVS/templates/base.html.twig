<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}
        <link rel="stylesheet" type="text/css" href="{{ asset('bootstrap-4.1.3-dist/css/bootstrap.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('css/style.css') }}">
    {% endblock %}
</head>
<body>
<div class="container_nav">
    {{ include('_nav.html.twig') }}
</div>
<div class="container_body">
    <div class="container">
        {% block body %}{% endblock %}
    </div>
</div>
{% block javascripts %}


    <script type="text/javascript" src="{{ asset('js/json2.js') }}"></script>
    <script type="text/javascript" src="{{ asset('js/classes/Tileset.js') }}"></script>
    <script type="text/javascript" src="{{ asset('js/classes/Map.js') }}"></script>
    <script type="text/javascript" src="{{ asset('js/classes/Personnage.js') }}"></script>
    <script type="text/javascript" src="{{ asset('js/classes/Monstre.js') }}"></script>
    <script type="text/javascript" src="{{ asset('js/rpg.js') }}"></script>
    <script type="text/javascript" src="{{ asset('js/XMLHttpRequest.js') }}"></script>
    <script src="{{ asset('jquery-3.3.1.slim.min.js') }}"></script>
    <script src="{{ asset('bootstrap-4.1.3/js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('popper.min.js') }}"></script>
    <script src="{{ asset('main.js') }}"></script>
{% endblock %}
</body>
</html>